package com.renren.ugc.comment.cache;


/**
 * 标注:{@link CommentTaskModel}  封装缓存key-value
 * 
 * @author xinyan.yang@renrne-inc.com
 * 
 * @data 2013-1-30 11:19:27
 *
 */
public class CommentTaskModel {
    
    private String key;
    
    private Object value;

    
    public CommentTaskModel() {

    }
    
    public CommentTaskModel(String key, Object value) {
        this.key = key;
        this.value = value;
    }
    public String getKey() {
        return key;
    }

    
    public void setKey(String key) {
        this.key = key;
    }

    
    public Object getValue() {
        return value;
    }

    
    public void setValue(Object value) {
        this.value = value;
    }
    
    
    
}
