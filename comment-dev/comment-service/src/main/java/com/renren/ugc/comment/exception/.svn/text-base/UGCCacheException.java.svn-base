package com.renren.ugc.comment.exception;


/**
 * 标注:{@link UGCCacheException} 封装评论中心cache异常
 * 
 * @author xinyan.yang@renren-inc.com
 *
 */
public class UGCCacheException extends RuntimeException {
    
    
    private static final long serialVersionUID = -8527601492814523053L;

    public UGCCacheException() {
        super();
    }
    
    public UGCCacheException(String message) {
        super(message);
    }
    
    
    public UGCCacheException(String message, Throwable cause) {
        super(message, cause);
    }
    
    public UGCCacheException(Throwable cause) {
        super(cause);
    }
    
    
}
