#############################################Common Basics #############################

##########alarm config
#####################alarm phones number config, please add your message separated by comma
phone=13661226467
#####################alarm emails  config, please add your message separated by comma
email=chao.wang9@renren-inc.com
##############base config,please change to your message topic, and you may connect with the admin to make sure
##############the topic is legal
topic=ugc_kafka_xlog
###########high level config, you don't need care it
consumer.partition.align=0
#########selializer type: java, commonlang
selializer.type=commonlang
##################zk config,please set your zk servers' parameters
zk.connect=userzookeeper1.d.xiaonei.com:2181,userzookeeper2.d.xiaonei.com:2181,userzookeeper3.d.xiaonei.com:2181,userzookeeper4.d.xiaonei.com:2181,userzookeeper5.d.xiaonei.com:2181/commentKafka
zk.connectiontimeout.ms=1000000
zk.synctime.ms=200
autocommit.interval.ms=100000
############################# Consumer Basics #############################
##########the parameter is your consumer groupid , the group can be processes or threads
groupid=wangxxtest
#####################consumer exception config
consumer.exception.switch=1
#############################if consumer.exception.switch=1
consumer.reconnect.time=30
consumer.clean.time=15
consumer.total.exception=100
consumer.exception.title=jade kafka consumer alarm
############################
consumer.rollback=0
consumer.update.number=10
#consumer.buffer.size=65536
#consumer.fetch.message.size=10240
#######################################if consumer.rollback=1
consumer.offset.type=FILE
############################# Producer Basics #############################
partitioner.class=com.renren.ad.infra.producer.InfraKafkaPartition
#partitioner.class=com.renren.ad.infra.producer.DefaultPartition
producer.type=async
compression.codec=0
serializer.class=kafka.serializer.DefaultEncoder
queue.time=50000
queue.size=1000
batch.size=200

#####################producer exception config
producer.exception.switch=1
#############################if producer.exception.switch=1 
producer.reconnect.time=30
producer.clean.time=15
producer.total.exception=100
producer.exception.title=jade kafka producer alarm
#############partition if  partitioner.class=com.renren.ad.infra.producer.InfraKafkaPartition
producer.broker.number=2
producer.per.broker.partition=1


